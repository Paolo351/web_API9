@model web_API9.Models.ViewModels.Show_all_databaseViewModel
@{
    ViewData["Title"] = "Show_all_database";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="text-left">
    <h1 class="display-4">Databases: </h1>
    <ul>
        @foreach (var Database in Model.Databases)
        {
            <li><b>@Database.DatabaseId</b>, Database name: <b>@Database.Name</b>, Engine: <b>@Database.Engine</b></li>


        }
    </ul>
    <form>
        <a class="nav-link text-light" asp-area="" asp-controller="Database" asp-action="Show_database">Back</a>
    </form>
</div>
