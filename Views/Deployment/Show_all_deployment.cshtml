@model web_API9.Models.ViewModels.Show_all_deploymentViewModel

@{
    ViewData["Title"] = "Show_all_deployment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="text-left" >
    <h1 class="display-4">Deployments: </h1>
    <ul>
        @foreach (var Deployment in Model.Deployment_RS)
        {
        <li><b>@Deployment.DeploymentId</b>, Name: <b>@Deployment.Name</b>, Time: <b>@Deployment.TimeOfDeployment.ToShortDateString()</b>, Database: <b>@Deployment.Nazwa_bd</b>, Project: <b>@Deployment.Nazwa_project</b>, User: <b>@Deployment.Nazwa_user</b></li>


        }
    </ul>
    <form>
        <a class="nav-link text-light" asp-area="" asp-controller="Deployment" asp-action="Show_deployment">Back</a>
    </form>
</div>
